.nav {
  font-size: $font-size-nav;
}

.nav>a {
  display: inline-block;
  //font-size: $font-size-popup-title;
}

.nav>a:after {
  font-family: "iconfont" !important;
  content: '\e6a8';
  margin-left: .1rem;
  margin-right: .05rem;
  color: $color-gray;
}

.nav>a:last-child:after {
  content: '';
}

.nav>label {
  color: $color-desc;
}
